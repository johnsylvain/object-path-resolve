{
  "name": "object-path-resolve",
  "version": "1.0.5",
  "description": "Utility function to resolve object paths",
  "source": "index.js",
  "module": "dist/index.es.js",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "repository": "https://github.com/johnsylvain/object-path-resolve.git",
  "author": "John Sylvain <john.n.sylvain@gmail.com>",
  "license": "MIT",
  "bundlesize": [
    {
      "path": "dist/index.js",
      "maxSize": "500 b"
    }
  ],
  "scripts": {
    "build": "npm-run-all --silent -p build:main -s size ",
    "build:main": "microbundle --entry index.ts",
    "size": "strip-json-comments --no-whitespace dist/index.js | gzip-size && bundlesize",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "release": "npm t && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish"
  },
  "devDependencies": {
    "@types/jest": "^23.1.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bundlesize": "^0.15.3",
    "gzip-size-cli": "^2.1.0",
    "jest": "^22.0.4",
    "microbundle": "^0.2.4",
    "npm-run-all": "^4.1.2",
    "strip-json-comments-cli": "^1.0.1",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.2"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(test.ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
